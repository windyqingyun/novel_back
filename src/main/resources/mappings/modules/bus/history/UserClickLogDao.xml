  <select id="findClikCount" parameterType="com.jeeplus.modules.bus.form.StatisticsForm" resultMap="BaseResultMap2">
    select 
		sum(l.click_num) as 'count',
		l.create_time as 'createTime',
		oo.name as 'originOffice',
		b.name as 'bookTitle',
		bo.name as 'bookOffice', 
		c.nickname as 'nickName'
	from user_click_log l
	left join sys_office oo on oo.id = l.origin
	left join book b on b.id = l.click_resource
	left join sys_office bo on bo.id = b.office_id
	left join customer c on c.id = l.create_by
	<where>
		1 = 1
		<if test="title != null and title != ''">
			and b.name like concat('%',#{title},'%')
		</if>
		<if test="formNickName != null and formNickName != ''">
			and c.nickname like concat('%',#{formNickName},'%')
		</if>
		<if test="bookOfficeId != null and bookOfficeId != ''">
			and b.office_id = #{bookOfficeId}
		</if>
		<if test="customerOfficeId != null and customerOfficeId != ''">
			and c.office_id = #{customerOfficeId}
		</if>
		<if test="office != null and office.id != null and office.id != ''">
			and b.office_id = #{office.id}
		</if>
		<if test="channelOffice != null and channelOffice.id != null and channelOffice.id != ''">
			and l.origin = #{channelOffice.id}
		</if>
		<if test="beginDate != null">
			<![CDATA[ AND l.create_time >= #{beginDate} ]]>
		</if>
		<if test="endDate != null">
			<![CDATA[ AND l.create_time < DATE_ADD(#{endDate},INTERVAL 1 DAY)  ]]>
		</if>
